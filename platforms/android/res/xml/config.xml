<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ibm.watson.WatsonPhotographyHybrid" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <name>Watson Photography</name>
    <description>
        A sample hybrid application for the IBM Watson Mobile Developer Challenge.
    </description>
    <author email="capaez@us.ibm.com" href="">
        IBM
    </author>
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <content src="index.html" />
    <access origin="*" />
</widget>
